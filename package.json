    "drizzle-orm": "^0.45.1",
    "nuxt": "^4.2.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.12.1",
    "eslint": "^9.39.1",
    "vue-tsc": "^3.1.8",
    "wrangler": "4.54.0"
  },
  "packageManager": "pnpm@10.25.0",
